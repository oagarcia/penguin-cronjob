{
  "name": "penguin-cronjob",
  "version": "1.0.0",
  "description": "Cron Job scheduler for penguin reports",
  "author": "Andres Garcia",
  "license": "ISC",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "start": "DEBUG=main node lib",
    "package": "zip -r ./penguin-cronjob_`date +%y.%m.%d`.zip package.json node_modules lib",
    "debug": "node --inspect --debug-brk lib",
    "prod": "npm install --production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oagarcia/penguin-cronjob"
  },
  "keywords": [
    "penguin",
    "basecamp",
    "timesheet",
    "zemoga"
  ],
  "dependencies": {
    "debug": "^2.6.3",
    "node-fetch": "^2.6.7",
    "node-schedule": "^1.2.1"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-defaults": "^9.0.0"
  }
}
